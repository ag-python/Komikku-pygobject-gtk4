<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="ApplicationWindow" parent="AdwApplicationWindow">
    <child>
      <object class="GtkBox" id="box">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkRevealer" id="headerbar_revealer">
            <property name="transition_type">none</property>
            <property name="reveal_child">1</property>
            <child>
              <object class="AdwHeaderBar" id="headerbar">
                <child type="title">
                  <object class="GtkStack" id="title_stack">
                    <property name="hexpand">1</property>
                    <property name="hhomogeneous">0</property>
                    <property name="transition_type">slide-left-right</property>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">library</property>
                        <property name="child">
                          <object class="GtkBox">
                            <property name="valign">center</property>
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkLabel" id="library_title_label">
                                <property name="halign">center</property>
                                <property name="label" translatable="yes">Komikku</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="title"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="library_subtitle_label">
                                <property name="halign">center</property>
                                <property name="label" translatable="yes">Library</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="subtitle"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">card</property>
                        <property name="child">
                          <object class="AdwViewSwitcherTitle" id="card_viewswitchertitle">
                            <property name="stack">card_stack</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">reader</property>
                        <property name="child">
                          <object class="GtkBox">
                            <property name="valign">center</property>
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkLabel" id="reader_title_label">
                                <property name="label">Manga title</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="title"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="reader_subtitle_label">
                                <property name="label">Chapter title</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="subtitle"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">preferences</property>
                        <property name="child">
                          <object class="GtkBox">
                            <property name="valign">center</property>
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkLabel" id="preferences_title_label">
                                <property name="label" translatable="yes">Preferences</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="title"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="preferences_subtitle_label">
                                <property name="visible">0</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="subtitle"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">categories_editor</property>
                        <property name="child">
                          <object class="GtkLabel" id="categories_editor_title_label">
                            <property name="label" translatable="yes">Categories</property>
                            <style>
                              <class name="title"/>
                            </style>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">download_manager</property>
                        <property name="child">
                          <object class="GtkBox">
                            <property name="valign">center</property>
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkLabel" id="download_manager_title_label">
                                <property name="label" translatable="yes">Download Manager</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="title"/>
                                </style>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel" id="download_manager_subtitle_label">
                                <property name="visible">0</property>
                                <property name="ellipsize">end</property>
                                <property name="single_line_mode">1</property>
                                <style>
                                  <class name="subtitle"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">explorer</property>
                        <property name="child">
                          <object class="GtkLabel" id="explorer_title_label">
                            <property name="ellipsize">end</property>
                            <style>
                              <class name="title"/>
                            </style>
                          </object>
                        </property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="left_button">
                    <property name="receives_default">1</property>
                    <property name="icon-name">list-add-symbolic</property>
                  </object>
                </child>
                <child>
                  <object class="GtkStack">
                    <child>
                      <object class="GtkToggleButton" id="library_flap_reveal_button">
                        <property name="receives_default">1</property>
                        <property name="icon-name">user-bookmarks-symbolic</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child type="end">
                  <object class="GtkMenuButton" id="menu_button">
                    <property name="receives_default">1</property>
                    <property name="icon-name">open-menu-symbolic</property>
                  </object>
                </child>
                <child type="end">
                  <object class="GtkStack" id="right_button_stack">
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">library</property>
                        <property name="child">
                          <object class="GtkToggleButton" id="library_search_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">edit-find-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">card</property>
                        <property name="child">
                          <object class="GtkButton" id="card_resume_read_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">media-playback-start-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">reader</property>
                        <property name="child">
                          <object class="GtkButton" id="reader_fullscreen_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">view-fullscreen-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">download_manager</property>
                        <property name="child">
                          <object class="GtkButton" id="download_manager_start_stop_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">media-playback-start-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">explorer.search</property>
                        <property name="child">
                          <object class="GtkToggleButton" id="explorer_search_page_server_website_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">web-browser-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">explorer.card</property>
                        <property name="child">
                          <object class="GtkButton" id="explorer_card_page_add_read_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">list-add-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="name">explorer.servers</property>
                        <property name="child">
                          <object class="GtkToggleButton" id="explorer_servers_page_search_button">
                            <property name="receives_default">1</property>
                            <property name="icon-name">edit-find-symbolic</property>
                          </object>
                        </property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkOverlay" id="overlay">
            <property name="vexpand">1</property>
            <property name="child">
              <object class="GtkStack" id="stack">
                <property name="transition_type">slide-left-right</property>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">library</property>
                    <property name="child">
                      <object class="AdwFlap" id="library_flap">
                        <child>
                          <object class="GtkStack" id="library_stack">
                            <child>
                              <object class="GtkStackPage">
                                <property name="name">flowbox</property>
                                <property name="child">
                                  <object class="GtkBox">
                                    <property name="orientation">vertical</property>
                                    <child>
                                      <object class="GtkSearchBar" id="library_searchbar">
                                        <child>
                                          <object class="GtkBox">
                                            <property name="valign">center</property>
                                            <property name="hexpand">1</property>
                                            <child>
                                              <object class="GtkSearchEntry" id="library_searchentry">
                                                <property name="hexpand">1</property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkMenuButton" id="library_search_menu_button">
                                                <property name="receives_default">1</property>
                                                <property name="icon-name">go-down-symbolic</property>
                                              </object>
                                            </child>
                                            <style>
                                              <class name="search-box"/>
                                            </style>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <child>
                                      <object class="GtkScrolledWindow">
                                        <property name="vexpand">1</property>
                                        <property name="hscrollbar_policy">never</property>
                                        <child>
                                          <object class="GtkViewport">
                                            <child>
                                              <object class="GtkFlowBox" id="library_flowbox">
                                                <property name="homogeneous">1</property>
                                                <property name="min_children_per_line">2</property>
                                                <property name="max_children_per_line">30</property>
                                                <property name="selection_mode">none</property>
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkStackPage">
                                <property name="name">start_page</property>
                                <property name="child">
                                  <object class="GtkGrid" id="start_page_grid">
                                    <property name="halign">center</property>
                                    <property name="valign">center</property>
                                    <property name="vexpand">1</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="halign">center</property>
                                        <property name="valign">center</property>
                                        <property name="orientation">vertical</property>
                                        <property name="spacing">12</property>
                                        <child>
                                          <object class="GtkImage" id="app_logo">
                                            <property name="halign">center</property>
                                            <property name="valign">center</property>
                                            <property name="hexpand">0</property>
                                            <property name="vexpand">0</property>
                                            <property name="pixel_size">140</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" comments="The umbrella sentence of the application on the first start screen">Komikku</property>
                                            <attributes>
                                              <attribute name="weight" value="bold"></attribute>
                                              <attribute name="size" value="21000"></attribute>
                                            </attributes>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel" id="subtitle_label">
                                            <property name="label" translatable="yes" comments="The subtitle of the umbrella sentence in the first start screen. This is a sentence which gives the user a starting point what he can do if he opens the application for the first time.">A manga, comic and BD reader</property>
                                            <property name="use_markup">1</property>
                                            <property name="wrap">1</property>
                                            <style>
                                              <class name="dim-label"/>
                                            </style>
                                          </object>
                                        </child>
                                        <layout>
                                          <property name="column">0</property>
                                          <property name="row">0</property>
                                        </layout>
                                      </object>
                                    </child>
                                  </object>
                                </property>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child type="separator">
                          <object class="GtkSeparator">
                            <style>
                              <class name="sidebar"/>
                            </style>
                          </object>
                        </child>
                        <child type="flap">
                          <object class="GtkBox">
                            <property name="width_request">200</property>
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkLabel">
                                <property name="margin-start">12</property>
                                <property name="margin-end">12</property>
                                <property name="margin_top">12</property>
                                <property name="margin_bottom">12</property>
                                <property name="label" translatable="yes">Categories</property>
                                <attributes>
                                  <attribute name="weight" value="bold"></attribute>
                                </attributes>
                              </object>
                            </child>
                            <child>
                              <object class="GtkSeparator">
                              </object>
                            </child>
                            <child>
                              <object class="GtkStack" id="library_categories_stack">
                                <property name="vexpand">1</property>
                                <child>
                                  <object class="GtkStackPage">
                                    <property name="name">list</property>
                                    <property name="child">
                                      <object class="GtkScrolledWindow">
                                        <property name="hscrollbar_policy">never</property>
                                        <property name="child">
                                          <object class="GtkViewport">
                                            <property name="child">
                                              <object class="GtkListBox" id="library_categories_listbox">
                                              </object>
                                            </property>
                                          </object>
                                        </property>
                                      </object>
                                    </property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkStackPage">
                                    <property name="name">empty</property>
                                    <property name="child">
                                      <object class="AdwStatusPage">
                                        <property name="icon_name">user-bookmarks-symbolic</property>
                                        <property name="title">No categories</property>
                                      </object>
                                    </property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox" id="library_categories_edit_mode_buttonbox">
                                <property name="homogeneous">1</property>
                                <property name="margin-start">6</property>
                                <property name="margin-end">6</property>
                                <property name="margin_top">6</property>
                                <property name="margin_bottom">6</property>
                                <property name="spacing">6</property>
                                <child>
                                  <object class="GtkButton" id="library_categories_edit_mode_cancel_button">
                                    <property name="label" translatable="yes">Cancel</property>
                                    <property name="receives_default">1</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="library_categories_edit_mode_ok_button">
                                    <property name="label" translatable="yes">OK</property>
                                    <property name="receives_default">1</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <style>
                              <class name="background"/>
                            </style>
                          </object>
                        </child>
                      </object>
                    </property>
                  </object>
                </child>
                <child>
                  <object class="GtkStackPage">
                    <property name="name">card</property>
                    <property name="child">
                      <object class="GtkBox" id="card_box">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="AdwViewStack" id="card_stack">
                            <property name="vexpand">1</property>
                            <child>
                              <object class="AdwViewStackPage">
                                <property name="name">info</property>
                                <property name="title" translatable="yes">Info</property>
                                <property name="icon_name">preferences-system-details</property>
                                <property name="child">
                                  <object class="GtkScrolledWindow">
                                    <property name="hscrollbar_policy">never</property>
                                    <property name="propagate_natural_width">1</property>
                                    <property name="child">
                                      <object class="GtkViewport">
                                        <child>
                                          <object class="AdwClamp">
                                            <property name="maximum_size">800</property>
                                            <child>
                                              <object class="GtkBox" id="card_info_box">
                                                <property name="orientation">vertical</property>
                                                <property name="spacing">6</property>
                                                <child>
                                                  <object class="GtkLabel" id="card_name_label">
                                                    <property name="margin-start">6</property>
                                                    <property name="margin-end">6</property>
                                                    <property name="margin_top">6</property>
                                                    <property name="margin_bottom">6</property>
                                                    <property name="label">name</property>
                                                    <property name="justify">center</property>
                                                    <property name="wrap">1</property>
                                                    <attributes>
                                                      <attribute name="weight" value="bold"></attribute>
                                                      <attribute name="size" value="18000"></attribute>
                                                    </attributes>
                                                  </object>
                                                </child>
                                                <child>
                                                  <object class="GtkGrid" id="card_info_grid">
                                                    <property name="margin-start">6</property>
                                                    <property name="margin-end">6</property>
                                                    <property name="row_spacing">6</property>
                                                    <property name="column_spacing">6</property>
                                                    <property name="column_homogeneous">1</property>
                                                    <child>
                                                      <object class="GtkImage" id="card_cover_image">
                                                        <layout>
                                                          <property name="column">0</property>
                                                          <property name="row">0</property>
                                                          <property name="row-span">12</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_authors_label_label">
                                                        <property name="label" translatable="yes">Authors</property>
                                                        <property name="xalign">0</property>
                                                        <attributes>
                                                          <attribute name="weight" value="bold"></attribute>
                                                        </attributes>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">0</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_authors_value_label">
                                                        <property name="label">authors value</property>
                                                        <property name="wrap">1</property>
                                                        <property name="xalign">0</property>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">1</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_genres_label_label">
                                                        <property name="label" translatable="yes">Genres</property>
                                                        <property name="xalign">0</property>
                                                        <attributes>
                                                          <attribute name="weight" value="bold"></attribute>
                                                        </attributes>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">2</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_genres_value_label">
                                                        <property name="label">genres value</property>
                                                        <property name="wrap">1</property>
                                                        <property name="xalign">0</property>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">3</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_status_label_label">
                                                        <property name="label" translatable="yes">Status</property>
                                                        <property name="xalign">0</property>
                                                        <attributes>
                                                          <attribute name="weight" value="bold"></attribute>
                                                        </attributes>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">4</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_status_value_label">
                                                        <property name="label">status value</property>
                                                        <property name="wrap">1</property>
                                                        <property name="xalign">0</property>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">5</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_server_label_label">
                                                        <property name="label" translatable="yes">Server</property>
                                                        <property name="xalign">0</property>
                                                        <attributes>
                                                          <attribute name="weight" value="bold"></attribute>
                                                        </attributes>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">8</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_server_value_label">
                                                        <property name="label">server value</property>
                                                        <property name="wrap">1</property>
                                                        <property name="xalign">0</property>
                                                        <layout>
                                                          <property name="column">1</property>
                                                          <property name="row">9</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_synopsis_label_label">
                                                        <property name="halign">start</property>
                                                        <property name="label" translatable="yes">Synopsis</property>
                                                        <attributes>
                                                          <attribute name="weight" value="bold"></attribute>
                                                        </attributes>
                                                        <layout>
                                                          <property name="column">0</property>
                                                          <property name="row">12</property>
                                                          <property name="column-span">2</property>
                                                        </layout>
                                                      </object>
                                                    </child>
                                                    <child>
                                                      <object class="GtkLabel" id="card_synopsis_value_label">
