<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkAboutDialog" id="about_dialog">
    <property name="program_name">@prettyname@</property>
    <property name="version">@VERSION@</property>
    <property name="copyright">© 2019-2021 @authorfullname@</property>
    <property name="comments" translatable="yes">A manga reader for GNOME

Never forget, you can support the authors
by buying the official comics when they are
available in your region/language.</property>
    <property name="website">@PACKAGE_URL@</property>
    <property name="website_label" translatable="yes">Learn more about Komikku</property>
    <property name="authors"></property>
    <property name="translator_credits"></property>
    <property name="logo_icon_name">@APPID@</property>
    <property name="license_type">gpl-3-0</property>
  </object>
</interface>
